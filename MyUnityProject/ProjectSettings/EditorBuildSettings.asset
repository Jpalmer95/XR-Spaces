%YAML 1.1
%TAG !u! tag:unity3d.com,2011:
--- !u!114 &1
EditorBuildSettings:
  m_ObjectHideFlags: 0
  scenes:
  - enabled: 0 # Disabled SampleScene
    path: Assets/Scenes/SampleScene.unity
  - enabled: 1 # Enabled MainRoom
    path: Assets/Scenes/MainRoom.unity
  platforms:
    WebGL:
      buildSystem: Default # Internal for older versions
      enabled: 1
      buildTarget: WebGL
    Standalone: # Example of another platform, can be removed if not needed
      buildSystem: Default
      enabled: 1
      buildTarget: StandaloneWindows64
# Add other platforms as needed, e.g., Android, iOS
